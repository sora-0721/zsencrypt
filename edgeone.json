{
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        },
        {
            "source": "/_next/static/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/apply*",
            "destination": "/",
            "statusCode": 301
        },
        {
            "source": "/documents*",
            "destination": "/docs/",
            "statusCode": 301
        }
    ]
}