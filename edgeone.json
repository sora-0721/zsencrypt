{
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=43200"
                }
            ]
        },
        {
            "source": "/_next/static/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/apply*",
            "destination": "/",
            "statusCode": 308
        },
        {
            "source": "/docuuments*",
            "destination": "/docs/",
            "statusCode": 308
        }
    ]
}